        ATagParams = class="sub-navigation__link _sub-level-2 _sub"
    }
    ACTIFSUB < .IFSUB
    ACTIFSUB{
        ATagParams = class="sub-navigation__link _sub-level-2 _active _sub"
    }
    CURIFSUB < .ACTIFSUB
    CURIFSUB {
        ATagParams = class="sub-navigation__link _sub-level-2 _curent _sub"
    }
}
